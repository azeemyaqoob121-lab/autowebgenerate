# Test Environment Configuration
# This file is used for running tests with an isolated test database

# Application
APP_NAME=AutoWeb Outreach AI Test
ENVIRONMENT=test
DEBUG=True
API_VERSION=0.1.0

# Database - Test Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/autoweb_test_db

# Security
SECRET_KEY=test-secret-key-for-testing-only-not-production-safe
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=1440

# CORS
CORS_ORIGINS=http://localhost:3000,http://localhost:3001
CORS_ALLOW_CREDENTIALS=True
CORS_ALLOW_METHODS=*
CORS_ALLOW_HEADERS=*

# Redis - Use separate database for tests
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=1

# Logging
LOG_LEVEL=DEBUG
